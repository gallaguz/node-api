{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "prisma": {
        "schema": "prisma/schema.prisma",
        "seed": "ts-node prisma/seed/index.ts"
    },
    "_moduleAliases": {
        "@app": "./dist"
    },
    "scripts": {
        "start": "node dist/main.js",
        "lint": "eslint ./src/**",
        "lint:fix": "eslint ./src/** --fix",
        "build": "tsc",
        "dev": "nodemon",
        "test": "jest --coverage",
        "test:e2e": "jest --config jest.e2e.config.ts --coverage",
        "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
        "debug": "nodemon --exec node --inspect-brk=0.0.0.0:9229 --require ts-node/register src/main.ts",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:seed": "npx prisma db seed",
        "prisma:deploy": "npx prisma migrate deploy",
        "prisma:reset": "npx prisma migrate reset"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^4.1.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dotenv": "^16.0.1",
        "express": "^4.18.0",
        "inversify": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "module-alias": "^2.2.2",
        "reflect-metadata": "^0.1.13",
        "tslog": "^3.3.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.30",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.4.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.3",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "prisma": "^4.1.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^4.1.0"
    }
}
